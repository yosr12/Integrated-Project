{% for question in questions %}
    <tr>
        <td>{{ question.id }}</td>
        <td>{{ question.question }}</td>
        <td><img src="{{ asset('uploads/'~question.image) }}" WIDTH=150 HEIGHT=120 alt="" />  </td>
        <td>{{ question.type }}</td>
        <td>
            <a href="{{ path('question_show', {'id': question.id}) }}">show</a>
            <a href="{{ path('question_edit', {'id': question.id}) }}">edit</a>
            <a href="{{ path('question_delete', {'id': question.id}) }}">Delete</a>
        </td>
        <td>

            <a href="{{  path('question_indexxx', {'idqs': question.id}) }}">afficher stat</a>

        </td>
    </tr>
{% else %}
    <tr>
        <td colspan="5">no records found</td>
    </tr>
{% endfor %}